<?xml version="1.0" ?>
<node name="/org/freedesktop/DBus/lge/testSW">
  <interface name="org.freedesktop.DBus.TestAgent">
    <method name="HelloString">
      <arg type="s" name="name" direction="in"/>
      <arg type="s" name="greeting" direction="out"/>
    </method>
    <method name="reqTestFromClient">
      <arg type="v" name="requestMsg" direction="in"/>
      <arg type="v" name="responseMsg" direction="out"/>
    </method>
 
    <signal name="EchoCount">
      <arg type="y" name="count"/>
    </signal>
  </interface>
</node>
